{
  "name": "first-network-org1",
  "version": "1.0.0",

  "client": {
    "organization": "Org1",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },

  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer0.org1.ownify.com",
        "peer1.org1.ownify.com"
      ],
      "certificateAuthorities": [
        "ca.org1.ownify.com"
      ]
    }
  },

  "peers": {
    "peer0.org1.ownify.com": {
      "url": "grpcs://peer0.org1.ownify.com:7051",
      "tlsCACerts": {
        "pem": "<<<TLS_CA_PEM_HERE>>>"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org1.ownify.com",
        "hostnameOverride": "peer0.org1.ownify.com"
      }
    },

    "peer1.org1.ownify.com": {
      "url": "grpcs://peer1.org1.ownify.com:8051",
      "tlsCACerts": {
        "pem": "<<<TLS_CA_PEM_HERE>>>"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer1.org1.ownify.com",
        "hostnameOverride": "peer1.org1.ownify.com"
      }
    }
  },

  "certificateAuthorities": {
    "ca.org1.ownify.com": {
      "url": "https://ca.org1.ownify.com:7054",
      "caName": "ca.org1.ownify.com",
      "tlsCACerts": {
        "pem": "<<<FABRIC_CA_SERVER_TLS_PEM>>>"
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
